package com.acron.device;

import java.util.TimerTask;

public class AssignDevice extends TimerTask{
	
	private int times = 0;
	
	@Override
	public void run() {
		times++;
		
		if( times < 99 ){
			System.out.println( times + " . cihaz atamasÄ±");
			AssignWorkflowToOnlineDevice job = new AssignWorkflowToOnlineDevice();
		}else{
			this.cancel();
		}
		
	}


}
